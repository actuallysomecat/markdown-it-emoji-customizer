{
  "name": "markdown-it-emoji-customizer",
  "version": "1.0.0",
  "description": "A helper module for markdown-it-emoji that enables the use of custom emoji shortcodes defined by scanning a path for emoji images, definition merging or overwriting, custom attributes, and configurable rendering options.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "lint-init": "eslint --init",
    "lint": "eslint index.js",
    "test": "npm run lint && c8 --exclude dist --exclude test -r text mocha",
    "test-cleanup": "rimraf coverage"
  },
  "keywords": [
    "markdownit",
    "markdown-it",
    "markdown",
    "emoji",
    "11ty",
    "eleventy",
    "plugin",
    "obsidian"
  ],
  "author": "cat",
  "license": "MIT",
  "homepage": "https://github.com/actuallysomecat/markdown-it-emoji-customizer",
  "bugs": "https://github.com/actuallysomecat/markdown-it-emoji-customizer/issues",
  "repository": "github:actuallysomecat/markdown-it-emoji-customizer",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.26.0",
    "c8": "^10.1.3",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "globals": "^16.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-testgen": "^0.1.6",
    "mocha": "^11.2.2",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "globby": "^14.1.0"
  },
  "peerDependencies": {
    "markdown-it-emoji": "^3.0.0"
  }
}
